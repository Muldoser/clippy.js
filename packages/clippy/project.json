{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/clippy/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/clippy",
        "main": "packages/clippy/src/index.js",
        "tsConfig": "packages/clippy/tsconfig.lib.json",
        "assets": ["packages/clippy/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/clippy/**/*.js"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/clippy"],
      "options": {
        "jestConfig": "packages/clippy/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
